	"styles": {
		"blocks": {
			"core/separator": {
				"border": {
					"color": "var(--wp--preset--color--tertiary)",
					"width": "2px"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "normal",
					"fontWeight": "600"
				}
			}
		},
		"elements": {
			"button": {
				":active": {
					"border": {
						"color": "var(--wp--preset--color--contrast)"
					},
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--tertiary)"
					}
				},
				":focus": {
					"border": {
						"color": "var(--wp--preset--color--contrast)"
					},
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--tertiary)"
					}
				},
				":hover": {
					"border": {
						"color": "var(--wp--preset--color--contrast)"
					},
					"color": {
						"background": "var(--wp--preset--color--contrast)",
						"text": "var(--wp--preset--color--tertiary)"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--base)"
					}
				},
				"border": {
					"color": "var(--wp--preset--color--primary)",
					"radius": "0",
					"style": "solid",
					"width": "2px"
				},
				"color": {
					"background": "var:preset|color|secondary",
					"text": "var:preset|color|tertiary"
				},
				"spacing": {
					"padding": {
						"bottom": "min(1.125rem, 3vw) !important",
						"left": "min(2.125rem, 5vw) !important",
						"right": "min(2.125rem, 5vw) !important",
						"top": "min(1.125rem, 3vw) !important"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "600",
					"letterSpacing": "0.01em",
					"textTransform": "uppercase"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "1.1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "1.1"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "500"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var:preset|color|light-green-cyan"
					}
				},
				"color": {
					"text": "var:preset|color|contrast"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--40)",
			"padding": {
				"left": "var(--wp--preset--spacing--70)",
				"right": "var(--wp--preset--spacing--70)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.7"
		}
	},
	"title": "mrwriter",
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.2/theme.json"
}